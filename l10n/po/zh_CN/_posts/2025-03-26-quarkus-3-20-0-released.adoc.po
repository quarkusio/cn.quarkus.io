msgid "https://github.com/quarkusio/quarkus/wiki/Migration-Guide-3.16[Migration guide for 3.16]"
msgstr ""

#: _posts/2025-03-26-quarkus-3-20-0-released.adoc
msgid "https://github.com/quarkusio/quarkus/wiki/Migration-Guide-3.17[Migration guide for 3.17]"
msgstr ""

#: _posts/2025-03-26-quarkus-3-20-0-released.adoc
msgid "https://github.com/quarkusio/quarkus/wiki/Migration-Guide-3.18[Migration guide for 3.18]"
msgstr ""

#: _posts/2025-03-26-quarkus-3-20-0-released.adoc
msgid "https://github.com/quarkusio/quarkus/wiki/Migration-Guide-3.19[Migration guide for 3.19]"
msgstr ""

#: _posts/2025-03-26-quarkus-3-20-0-released.adoc
msgid "https://github.com/quarkusio/quarkus/wiki/Migration-Guide-3.20[Migration guide for 3.20] - this one is empty as 3.20 is the continuation of 3.19"
msgstr ""

