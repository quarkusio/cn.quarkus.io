#. type: delimited block -
#: upstream/_versions/3.0/guides/redis.adoc:132
#, no-wrap
msgid "    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/redis.adoc:141
#, no-wrap
msgid ""
"    long get(String key) {\n"
"        Long value = countCommands.get(key); // <6>\n"
"        if (value == null) {\n"
"            return 0L;\n"
"        }\n"
"        return value;\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/redis.adoc:145
#, no-wrap
msgid ""
"    void set(String key, Long value) {\n"
"        countCommands.set(key, value); // <7>\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/redis.adoc:149
#, no-wrap
msgid ""
"    void increment(String key, Long incrementBy) {\n"
"        countCommands.incrby(key, incrementBy); // <8>\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/redis.adoc:154
#, no-wrap
msgid ""
"    Uni<Void> del(String key) {\n"
"        return keyCommands.del(key) // <9>\n"
"            .replaceWithVoid();\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/redis.adoc:159
#, no-wrap
msgid ""
"    Uni<List<String>> keys() {\n"
"        return keyCommands.keys(\"*\"); // <10>\n"
"    }\n"
"}\n"
msgstr ""

