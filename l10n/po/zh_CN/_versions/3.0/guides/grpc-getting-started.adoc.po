#. type: Title ===
#: upstream/_versions/3.0/guides/grpc-getting-started.adoc:199
#, no-wrap
msgid "Implementing a service"
msgstr ""

#. type: Plain text
#: upstream/_versions/3.0/guides/grpc-getting-started.adoc:202
msgid "Create the `src/main/java/org/acme/HelloService.java` file with the following content:"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/grpc-getting-started.adoc:206
#: upstream/_versions/3.0/guides/grpc-getting-started.adoc:233
#: upstream/_versions/3.0/guides/grpc-getting-started.adoc:283
#, no-wrap
msgid "package org.acme;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/grpc-getting-started.adoc:212
#, no-wrap
msgid ""
"import io.quarkus.example.Greeter;\n"
"import io.quarkus.example.HelloReply;\n"
"import io.quarkus.example.HelloRequest;\n"
"import io.quarkus.grpc.GrpcService;\n"
"import io.smallrye.mutiny.Uni;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/grpc-getting-started.adoc:215
#, no-wrap
msgid ""
"@GrpcService <1>\n"
"public class HelloService implements Greeter {  <2>\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/grpc-getting-started.adoc:223
#, no-wrap
msgid ""
"    @Override\n"
"    public Uni<HelloReply> sayHello(HelloRequest request) { <3>\n"
"        return Uni.createFrom().item(() ->\n"
"                HelloReply.newBuilder().setMessage(\"Hello \" + request.getName()).build()\n"
"        );\n"
"    }\n"
"}\n"
msgstr ""

#. type: Plain text
#: upstream/_versions/3.0/guides/grpc-getting-started.adoc:225
#: upstream/_versions/3.0/guides/grpc-getting-started.adoc:253
msgid "Expose your implementation as a bean."
msgstr ""

#. type: Plain text
#: upstream/_versions/3.0/guides/grpc-getting-started.adoc:226
msgid "Implement the generated service interface."
msgstr ""

#. type: Plain text
#: upstream/_versions/3.0/guides/grpc-getting-started.adoc:227
#: upstream/_versions/3.0/guides/grpc-getting-started.adoc:255
msgid "Implement the methods defined in the service definition (here we have a single method)."
msgstr ""

#. type: Plain text
#: upstream/_versions/3.0/guides/grpc-getting-started.adoc:229
msgid "You can also use the default gRPC API instead of Mutiny:"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/grpc-getting-started.adoc:239
#, no-wrap
msgid ""
"import io.grpc.stub.StreamObserver;\n"
"import io.quarkus.example.GreeterGrpc;\n"
"import io.quarkus.example.HelloReply;\n"
"import io.quarkus.example.HelloRequest;\n"
"import io.quarkus.grpc.GrpcService;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/grpc-getting-started.adoc:242
#, no-wrap
msgid ""
"@GrpcService <1>\n"
"public class HelloService extends GreeterGrpc.GreeterImplBase { <2>\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/grpc-getting-started.adoc:251
#, no-wrap
msgid ""
"    @Override\n"
"    public void sayHello(HelloRequest request, StreamObserver<HelloReply> responseObserver) { <3>\n"
"        String name = request.getName();\n"
"        String message = \"Hello \" + name;\n"
"        responseObserver.onNext(HelloReply.newBuilder().setMessage(message).build()); <4>\n"
"        responseObserver.onCompleted(); <5>\n"
"    }\n"
"}\n"
msgstr ""

#. type: Plain text
#: upstream/_versions/3.0/guides/grpc-getting-started.adoc:254
msgid "Extends the `ImplBase` class. This is a generated class."
