#. type: delimited block -
#: upstream/_versions/3.0/guides/spring-data-rest.adoc:217
#, no-wrap
msgid ""
"import static io.restassured.RestAssured.given;\n"
"import static org.hamcrest.CoreMatchers.containsString;\n"
"import static org.hamcrest.CoreMatchers.notNullValue;\n"
"import static org.hamcrest.core.IsNot.not;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/spring-data-rest.adoc:220
#, no-wrap
msgid ""
"@QuarkusTest\n"
"class FruitsRepositoryTest {\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/spring-data-rest.adoc:234
#, no-wrap
msgid ""
"    @Test\n"
"    void testListAllFruits() {\n"
"        //List all, should have all 3 fruits the database has initially:\n"
"        given()\n"
"                .accept(\"application/json\")\n"
"                .when().get(\"/fruits\")\n"
"                .then()\n"
"                .statusCode(200)\n"
"                .body(\n"
"                        containsString(\"Cherry\"),\n"
"                        containsString(\"Apple\"),\n"
"                        containsString(\"Banana\")\n"
"                );\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/spring-data-rest.adoc:240
#, no-wrap
msgid ""
"        //Delete the Cherry:\n"
"        given()\n"
"                .when().delete(\"/fruits/1\")\n"
"                .then()\n"
"                .statusCode(204);\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/spring-data-rest.adoc:252
#, no-wrap
msgid ""
"        //List all, cherry should be missing now:\n"
"        given()\n"
"                .accept(\"application/json\")\n"
"                .when().get(\"/fruits\")\n"
"                .then()\n"
"                .statusCode(200)\n"
"                .body(\n"
"                        not(containsString(\"Cherry\")),\n"
"                        containsString(\"Apple\"),\n"
"                        containsString(\"Banana\")\n"
"                );\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/spring-data-rest.adoc:264
#, no-wrap
msgid ""
"        //Create a new Fruit\n"
"        given()\n"
"                .contentType(\"application/json\")\n"
"                .accept(\"application/json\")\n"
"                .body(\"{\\\"name\\\": \\\"Orange\\\", \\\"color\\\": \\\"Orange\\\"}\")\n"
"                .when().post(\"/fruits\")\n"
"                .then()\n"
"                .statusCode(201)\n"
"                .body(containsString(\"Orange\"))\n"
"                .body(\"id\", notNullValue())\n"
"                .extract().body().jsonPath().getString(\"id\");\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/spring-data-rest.adoc:278
#, no-wrap
msgid ""
"        //List all, Orange should be present now:\n"
"        given()\n"
"                .accept(\"application/json\")\n"
"                .when().get(\"/fruits\")\n"
"                .then()\n"
"                .statusCode(200)\n"
"                .body(\n"
"                        not(containsString(\"Cherry\")),\n"
"                        containsString(\"Apple\"),\n"
"                        containsString(\"Orange\")\n"
"                );\n"
"    }\n"
"}\n"
msgstr ""

