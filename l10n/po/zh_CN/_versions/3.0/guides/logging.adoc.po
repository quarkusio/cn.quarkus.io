#: upstream/_versions/3.0/guides/logging.adoc:38
#, no-wrap
msgid ""
"import jakarta.ws.rs.GET;\n"
"import jakarta.ws.rs.Path;\n"
"import jakarta.ws.rs.Produces;\n"
"import jakarta.ws.rs.core.MediaType;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/logging.adoc:41
#, no-wrap
msgid ""
"@Path(\"/hello\")\n"
"public class ExampleResource {\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/logging.adoc:43
#, no-wrap
msgid "    private static final Logger LOG = Logger.getLogger(ExampleResource.class);\n"
msgstr ""

