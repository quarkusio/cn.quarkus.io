"    \"min\": 0.58961,\t\t\t\t\t\t\t\t\t\t                                            <3>\n"
"    \"mean\": 112.15909190834819,\t\t\t\t\t\t\t\t                                        <4>\n"
"    \"max\": 217.231273,\t\t\t\t\t\t\t\t\t                                            <5>\n"
"    \"stddev\": 108.2721053982776,\t\t\t\t\t\t\t    \t                                <6>\n"
"    \"count\": 2,\t\t\t\t\t\t\t\t\t\t\t                                            <7>\n"
