#. type: Title ==
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:58
#, no-wrap
msgid "GreetingController"
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:62
msgid "First, create a simple `GreetingResource` JAX-RS resource in the `src/main/java/org/acme/spring/cloud/config/client/GreetingResource.java` file that looks like:"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:66
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:88
#, no-wrap
msgid "package org.acme.spring.spring.cloud.config.client;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:71
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:93
#, no-wrap
msgid ""
"import javax.ws.rs.GET;\n"
"import javax.ws.rs.Path;\n"
"import javax.ws.rs.Produces;\n"
"import javax.ws.rs.core.MediaType;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:74
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:98
#, no-wrap
msgid ""
"@Path(\"/hello\")\n"
"public class GreetingResource {\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:81
#, no-wrap
msgid ""
"    @GET\n"
"    @Produces(MediaType.TEXT_PLAIN)\n"
"    public String hello() {\n"
"        return \"hello\";\n"
"    }\n"
"}\n"
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:84
msgid "As we want to use configuration properties obtained from the Config Server, we will update the `GreetingResource` to inject the `message` property. The updated code will look like this:"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:95
#, no-wrap
msgid "import org.eclipse.microprofile.config.inject.ConfigProperty;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:101
#, no-wrap
msgid ""
"    @ConfigProperty(name = \"message\", defaultValue=\"hello default\")\n"
"    String message;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:108
#, no-wrap
msgid ""
"    @GET\n"
"    @Produces(MediaType.TEXT_PLAIN)\n"
"    public String hello() {\n"
"        return message;\n"
"    }\n"
"}\n"
msgstr ""

#. type: Title ==
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:110
#, no-wrap
msgid "Configuring the application"
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:113
msgid "Quarkus provides various configuration knobs under the `quarkus.spring-cloud-config` root. For the purposes of this guide, our Quarkus application is going to be configured in `application.properties` as follows:"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:122
#, no-wrap
msgid ""
"# use the same name as the application name that was configured when standing up the Config Server\n"
"quarkus.application.name=a-bootiful-client\n"
"# enable retrieval of configuration from the Config Server - this is off by default\n"
"quarkus.spring-cloud-config.enabled=true\n"
"# configure the URL where the Config Server listens to HTTP requests - this could have been left out since http://localhost:8888 is the default\n"
"quarkus.spring-cloud-config.url=http://localhost:8888\n"
msgstr ""

