#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:124
#, no-wrap
msgid "Package and run the application"
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:127
msgid "Run the application with:"
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:131
msgid "Open your browser to http://localhost:8080/greeting."
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/spring-cloud-config-client.adoc:133
msgid "The result should be: `Hello world` as it is the value obtained from the Spring Cloud Config server."
msgstr ""

