#. type: Plain text
#: upstream/_versions/main/guides/logging.adoc:768
msgid "Quarkus overrides the logging Mapped Diagnostic Context (MDC) to improve the compatibility with its reactive core."
msgstr ""

#. type: Title ====
#: upstream/_versions/main/guides/logging.adoc:769
#, no-wrap
msgid "Add and read MDC data"
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/logging.adoc:772
msgid "To add data to the MDC and extract it in your log output:"
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/logging.adoc:774
msgid "Use the `MDC` class to set the data."
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/logging.adoc:775
msgid "Customize the log format to use `%X\\{mdc-key\\}`."
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/logging.adoc:777
msgid "Let's consider the following code:"
msgstr ""

#. type: Block title
#: upstream/_versions/main/guides/logging.adoc:779
#, no-wrap
msgid "Example with JBoss Logging and `io.quarkus.logging.Log`"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/logging.adoc:782
#, no-wrap
msgid "package me.sample;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/logging.adoc:787
#, no-wrap
msgid ""
"import io.quarkus.logging.Log;\n"
"import jakarta.ws.rs.GET;\n"
"import jakarta.ws.rs.Path;\n"
"import org.jboss.logmanager.MDC;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/logging.adoc:789
#, no-wrap
msgid "import java.util.UUID;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/logging.adoc:792
#, no-wrap
msgid ""
"@Path(\"/hello/jboss\")\n"
"public class GreetingResourceJbossLogging {\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/logging.adoc:802
#, no-wrap
msgid ""
"    @GET\n"
"    @Path(\"/test\")\n"
"    public String greeting() {\n"
"        MDC.put(\"request.id\", UUID.randomUUID().toString());\n"
"        MDC.put(\"request.path\", \"/hello/test\");\n"
"        Log.info(\"request received\");\n"
"        return \"hello world!\";\n"
"    }\n"
"}\n"
msgstr ""

#. type: Plain text
#: upstream/_versions/main/guides/logging.adoc:805
msgid "If you configure the log format with the following line:"
