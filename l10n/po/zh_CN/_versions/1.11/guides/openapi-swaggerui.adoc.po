"            schema:\n"
"              $ref: '#/components/schemas/Fruit'\n"
"      responses:\n"
"        200:\n"
"          description: OK\n"
"          content:\n"
"            application/json: {}\n"
"    delete:\n"
"      requestBody:\n"
"        content:\n"
"          application/json:\n"
"            schema:\n"
"              $ref: '#/components/schemas/Fruit'\n"
