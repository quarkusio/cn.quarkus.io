#. type: delimited block -
#: upstream/_versions/1.11/guides/spring-di.adoc:218
#, no-wrap
msgid ""
"import javax.ws.rs.GET;\n"
"import javax.ws.rs.Path;\n"
"import javax.ws.rs.Produces;\n"
"import javax.ws.rs.core.MediaType;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/1.11/guides/spring-di.adoc:221
#, no-wrap
msgid ""
"@Path(\"/greeting\")\n"
"public class GreeterResource {\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/1.11/guides/spring-di.adoc:224
#, no-wrap
msgid ""
"    @Autowired\n"
"    GreeterBean greeterBean;\n"
