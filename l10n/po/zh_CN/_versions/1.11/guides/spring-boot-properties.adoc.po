#. type: delimited block -
#: upstream/_versions/1.11/guides/spring-boot-properties.adoc:179
#: upstream/_versions/1.11/guides/spring-boot-properties.adoc:256
#, no-wrap
msgid "    public String text;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/1.11/guides/spring-boot-properties.adoc:182
#, no-wrap
msgid ""
"    public String suffix = \"!\";\n"
"}\n"
msgstr ""

#. type: Plain text
#: upstream/_versions/1.11/guides/spring-boot-properties.adoc:185
#: upstream/_versions/1.11/guides/spring-boot-properties.adoc:264
