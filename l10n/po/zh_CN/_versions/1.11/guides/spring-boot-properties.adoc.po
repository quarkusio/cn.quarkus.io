#: upstream/_versions/1.11/guides/spring-boot-properties.adoc:208
#, no-wrap
msgid ""
"    @GET\n"
"    @Produces(MediaType.TEXT_PLAIN)\n"
"    public String hello() {\n"
"        return properties.text + properties.suffix;\n"
"    }\n"
"}\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/1.11/guides/spring-boot-properties.adoc:216
