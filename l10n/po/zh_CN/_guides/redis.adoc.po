msgstr ""

#. type: delimited block -
#: upstream/_guides/redis.adoc:190
#, no-wrap
msgid ""
"@Path(\"/increments\")\n"
"public class IncrementResource {\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/redis.adoc:193
#, no-wrap
msgid ""
"    @Inject\n"
"    IncrementService service;\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/redis.adoc:198
#, no-wrap
msgid ""
"    @GET\n"
"    public Uni<List<String>> keys() {\n"
