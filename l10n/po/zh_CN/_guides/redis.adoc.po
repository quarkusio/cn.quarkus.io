#: upstream/_guides/redis.adoc:204
#, no-wrap
msgid ""
"    @POST\n"
"    public Increment create(Increment increment) {\n"
"        service.set(increment.key, increment.value);\n"
"        return increment;\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/redis.adoc:210
#, no-wrap
msgid ""
"    @GET\n"
