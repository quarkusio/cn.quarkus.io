#. type: delimited block -
#: upstream/_guides/redis.adoc:216
#, no-wrap
msgid ""
"    @PUT\n"
"    @Path(\"/{key}\")\n"
"    public void increment(String key, long value) {\n"
"        service.increment(key, value);\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/redis.adoc:223
#, no-wrap
msgid ""
"    @DELETE\n"
