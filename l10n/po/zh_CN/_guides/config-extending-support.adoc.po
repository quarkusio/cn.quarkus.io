#: _guides/config-extending-support.adoc
#, no-wrap
msgid "org.acme.config.LoggingConfigSourceInterceptor"
msgstr ""

#. type: Block title
#: _guides/config-extending-support.adoc
#, no-wrap
msgid "META-INF/services/io.smallrye.config.ConfigSourceInterceptor"
msgstr ""

