"                      </goals>\n"
"                      <configuration>\n"
"                        <exclClassLoaders>*QuarkusClassLoader</exclClassLoaders>  <1>\n"
"                        <destFile>${project.build.directory}/jacoco-quarkus.exec</destFile>\n"
"                        <append>true</append>\n"
