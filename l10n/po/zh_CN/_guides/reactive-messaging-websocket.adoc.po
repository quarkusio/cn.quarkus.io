#, no-wrap
msgid ""
"public class Cost {\n"
"    private double value;\n"
"    private String currency;\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/reactive-messaging-websocket.adoc:129
#, no-wrap
msgid ""
"    public Cost(double value, String currency) {\n"
"        this.value = value;\n"
"        this.currency = currency;\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/reactive-messaging-websocket.adoc:132
#, no-wrap
msgid ""
"    public Cost() {\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/reactive-messaging-websocket.adoc:136
#, no-wrap
msgid ""
"    public void setValue(double value) {\n"
"        this.value = value;\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/reactive-messaging-websocket.adoc:140
#, no-wrap
msgid ""
"    public void setCurrency(String currency) {\n"
"        this.currency = currency;\n"
"    }\n"
msgstr ""

#. type: delimited block -
