".application.properties\n"
"----\n"
"%dev.quarkus.hibernate-orm.sql-load-script = import-dev.sql\n"
"%test.quarkus.hibernate-orm.sql-load-script = import-test.sql\n"
"%prod.quarkus.hibernate-orm.sql-load-script = no-file\n"
"----\n"
"\n"
